.modelboxbg {
  position: absolute;
  top: 0;
  z-index: 10000;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.3;
}

.modelbox {
  position: absolute;
  bottom: 0;
  z-index: 999999;
  width: 100%;
  background: #fff;
}

.picker-view-column {
  text-align: center;
}

.model_picker {
  position: relative;
}

.button_model {
  height: 80rpx;
  width: 100%;
  background: #fff;
  position: relative;
  border-bottom: 1px solid #d9d9d9;
}

.button_model .cancel {
  color: #007aff;
  position: absolute;
  background: transparent;
  border: none;
  line-height: 80rpx;
  left: 32rpx;
}

.button_model .sure {
  color: #007aff;
  position: absolute;
  background: transparent;
  border: none;
  line-height: 80rpx;
  right: 32rpx;
}